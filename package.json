{
  "name": "codeclub-viewer",
  "version": "1.0.0",
  "description": "A viewer to display codeclub lessons",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server -d --progress --inline --hot --host 0.0.0.0",
    "build:debug": "webpack -d --progress --env.verbose",
    "build": "webpack --progress",
    "build:prod": "webpack -p --progress --env.NODE_ENV=production --env.BUILD_PDF",
    "build:analyse:": "webpack --progress --profile --json > stats.json",
    "build:travis": "webpack -p --env.NODE_ENV=production --env.BUILD_PDF",
    "serve": "node server.js",
    "test": "mocha \"test/**/*@(.js|.jsx)\"",
    "test:links": "node test-links.js",
    "test:watch": "yarn test -- --watch",
    "eslint": "eslint *.js src/ test/ --ext .js,.jsx",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "testall": "yarn eslint && yarn stylelint && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NorwegianKiwi/codeclub-viewer.git"
  },
  "keywords": [
    "codeclub",
    "lesson",
    "viewer"
  ],
  "author": "Lance Olav Eastgate",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NorwegianKiwi/codeclub-viewer/issues"
  },
  "homepage": "https://github.com/NorwegianKiwi/codeclub-viewer#readme",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "dependencies": {
    "fast-memoize": "^2.3.2",
    "highlight.js": "^9.11.0",
