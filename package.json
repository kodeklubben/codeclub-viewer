{
  "name": "codeclub-viewer",
  "version": "1.0.0",
  "description": "A viewer to display codeclub lessons",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server -d --progress --inline --hot --host 0.0.0.0",
    "build:debug": "webpack --mode=development -d --progress --env.verbose",
    "build": "webpack --mode=development --progress",
    "build:prod": "webpack --mode=production -p --progress --env.BUILD_PDF",
    "build:analyse": "webpack --mode=production --progress --profile --json > stats.json",
    "build:travis": "webpack --mode=production -p --env.BUILD_PDF",
    "serve": "node server.js",
    "test": "mocha \"test/**/*@(.js|.jsx)\"",
    "test:links": "node test-links.js",
    "test:watch": "yarn test -- --watch",
    "eslint": "eslint *.js src/ test/ --ext .js,.jsx",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "testall": "yarn eslint && yarn stylelint && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NorwegianKiwi/codeclub-viewer.git"
  },
  "keywords": [
    "codeclub",
    "lesson",
    "viewer"
  ],
  "author": "Lance Olav Eastgate",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NorwegianKiwi/codeclub-viewer/issues"
  },
  "browserslist": [
    "defaults"
  ],
  "homepage": "https://github.com/NorwegianKiwi/codeclub-viewer#readme",
