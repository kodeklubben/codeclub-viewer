{
  "name": "codeclub-viewer",
  "version": "1.0.0",
  "description": "A viewer to display codeclub lessons",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server -d --progress --inline --hot --history-api-fallback",
    "buildjs": "webpack -d --progress",
    "buildjs:prod": "cross-env NODE_ENV=production webpack -p --progress",
    "buildstatic": "webpack --config=webpack.static.config.babel.js --progress",
    "build": "npm run buildjs && npm run buildstatic",
    "build:prod": "npm run buildjs:prod && npm run buildstatic",
    "build:prod:travis": "cross-env NODE_ENV=production webpack -p && webpack --config=webpack.static.config.babel.js",
    "serve": "http-server ./dist",
    "servejs": "node ./server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js \"test/**/*@(.js|.jsx)\"",
    "test:watch": "npm run test -- --watch",
    "eslint": "eslint src/ test/ --ext .js,.jsx",
    "stylelint": "stylelint 'src/**/*.scss'",
    "testall": "npm run eslint && npm run stylelint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NorwegianKiwi/codeclub-viewer.git"
  },
  "keywords": [
    "codeclub",
    "lesson",
    "viewer"
  ],
  "author": "Lance Olav Eastgate",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NorwegianKiwi/codeclub-viewer/issues"
  },
  "homepage": "https://github.com/NorwegianKiwi/codeclub-viewer#readme",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "dependencies": {
    "react": "^15.1.0",
    "react-bootstrap": "^0.29.4",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-scroll": "^0.2.0",
    "redux": "^3.5.2",
    "reselect": "^2.5.1",
    "scratchblocks": "github:arve0/scratchblocks"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "bootstrap-loader": "^1.0.10",
    "bootstrap-sass": "^3.3.6",
    "bundle-loader": "^0.5.4",
    "case-sensitive-paths-webpack-plugin": "^1.1.2",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.9",
    "combine-loader": "^0.2.0",
    "compression": "^1.6.2",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "deep-freeze": "^0.0.1",
    "eslint": "^3.1.1",
    "eslint-plugin-react": "^5.2.2",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "front-matter-loader": "^0.1.2",
    "glob": "^7.0.3",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.19.0",
    "isomorphic-style-loader": "^1.0.0",
    "jsdom": "^9.2.1",
    "json-loader": "^0.5.4",
    "markdown-it-anchor": "^2.5.0",
    "markdown-it-attrs": "^0.5.1",
    "markdown-it-header-sections": "^0.2.2",
    "markdown-it-implicit-figures": "^0.3.0",
    "markdown-it-loader": "^0.2.0",
    "mocha": "^2.5.3",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.3.0",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.2.0",
    "static-site-generator-webpack-plugin": "^2.1.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.0.1",
    "stylelint-config-standard": "^11.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.0"
  }
}
